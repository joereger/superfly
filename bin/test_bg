#! /app/vendor/node/bin/node

var slack = require("../conf/slack.js");
var mongo = require("../conf/mongo.js");

function test_bg() {

  console.log('Hello from a scheduled task');

  mongo.SlackMessage.find(function (err, slack_messages) {
    if (err) return console.error(err);
    console.log(slack_messages)
  })

  console.log('Good bye from a scheduled task');

}
test_bg();